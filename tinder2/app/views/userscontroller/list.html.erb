<h1>users</h1>
<table>
    <tr>
        <td>name</td>
        <td>username</td>
        <td>email</td>
        <td>comuna</td>
    </tr>
    <% @users.each do |i|%>
    <tr>
        <td><%= i.name%></td>
        <td><%= i.username%></td>
        <td><%= i.email%></td>
        <td><%= i.comuna%></td>
    </tr>
    <%end%>
</table>
<%= link_to "create user", action:'create' %>