
<p id="notice"><%= notice %></p>

<h1>Gustos</h1>

<%= form_with(model: @gusto, method: :post) do |form|%>

    <div class = 'field'>
        <%= form.label :nombre %>
        <%= form.text_field :nombre %>
    </div>

    <div class = 'field'>
        <%= form.label :descripcion %>
        <%= form.text_field :descripcion %>
    </div>

    <div class='actions'>
        <%= form.submit %>
    </div>

<% end %>


